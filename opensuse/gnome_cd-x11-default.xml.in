<?xml version="1.0"?>
<test>
  <setup arch="GEN_ARCH">
    <source url="GEN_URL" name="tmp"/>
    <system file="solver-system.xml"/>
    <locale name="en_US"/>
  </setup>
  <trial>
    <!-- INCLUDE ignore_opensuse-bundle-kde.xml -->
    <!-- INCLUDE ignore_opensuse-bundle-gnome.xml -->
    <lock package="gtk2-32bit"/>
    <lock package="hpijs-standalone"/>
    <lock package="hplip-hpijs"/>
    <lock package="samba-client"/>
    <!-- INCLUDE ignore_opensuse.xml -->
    <!-- INCLUDE ignore_opensuse-cds.xml -->
    <!-- INCLUDE ignore_opensuse-gnome-cd.xml -->
    <!-- INCLUDE ignore_opensuse-basedon.xml -->
    <!-- INCLUDE ignore_opensuse-images.xml -->
    <install kind="package" name="kernel-default"/>
    <install kind="package" name="openSUSE-release-ftp"/>
    <install kind="pattern" name="x11"/>
    <lock package="kernel-firmware"/>
    <reportProblems/>
    <showpool verbose="true"/>
  </trial>
</test>
