<?xml version="1.0"?>
<test>
  <setup arch="GEN_ARCH">
    <source url="GEN_URL" name="tmp"/>
    <system file="solver-system.xml"/>
    <locale name="en_US"/>
    <locale name="de"/>
    <locale name="ru"/>
    <!-- locale name="it"/ -->
    <!-- locale name="es"/ -->
    <!-- locale name="pl"/ -->
    <!-- locale name="fr"/ -->
