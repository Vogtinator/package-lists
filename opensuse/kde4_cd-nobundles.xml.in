    <!-- INCLUDE ignore_opensuse.xml -->
    <!-- INCLUDE ignore_opensuse-cds.xml -->
    <!-- INCLUDE ignore_opensuse-kde-cd.xml -->
    <lock package="pam-32bit"/> <!-- !i586 !ppc -->
    <lock package="kdelibs3"/>
