<?xml version="1.0"?>
<test>
  <setup arch="GEN_ARCH">
    <source url="GEN_URL" name="tmp"/>
    <system file="solver-system.xml"/>
    <locale name="en_US"/>
    <locale name="en_GB"/>
    <locale name="pt"/>
    <locale name="pt_BR"/>
    <locale name="en"/>
    <locale name="de"/>
    <locale name="es"/>
    <locale name="it"/>
    <locale name="fr"/>
    <locale name="ja"/>
    <!-- Tier 2 -->
    <locale name="ru"/>
    <locale name="cs"/>
    <locale name="hu"/>
    <locale name="pl"/>
    <locale name="fi"/>
    <locale name="da"/>
    <locale name="nb"/>
    <locale name="sv"/>
    <locale name="nl"/>
    <locale name="zh"/>
    <locale name="zh_TW"/>
    <locale name="zh_CN"/>
