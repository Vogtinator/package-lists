    <install kind="package" name="openSUSE-release-dvd-promo"/>
<lock package="qt3"/> 
<lock package="gimp-help-it"/> 
<lock package="gimp-help-pl"/>
<lock package="gimp-help-nl"/>
<lock package="texlive-context"/>
<lock package="libqt4-devel-doc-data"/>
<lock package="icewm-default"/>
<lock package="kdeartwork4-wallpapers"/>

    <reportProblems/>
