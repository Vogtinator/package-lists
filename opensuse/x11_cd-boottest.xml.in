<?xml version="1.0"?>
<test>
  <setup arch="GEN_ARCH">
    <source url="GEN_URL" name="tmp"/>
    <system file="solver-system.xml"/>
    <locale name="en_US"/>
  </setup>
  <trial>
    <!-- ignoring some lang provides -->
    <!-- ignoring some recommends -->
    <lock package="oxygen-icon-theme-scalable"/>
    <lock package="spamassassin"/>
    <lock package="gutenprint"/>
    <lock package="rdesktop"/>
    <lock package="gimp"/>
    <lock package="ufraw"/>
    <!-- SLE themes -->
    <lock package="gnome-menus-branding-SLED"/>
    <lock package="gtk2-branding-SLED"/>
    <lock package="gtk2-theme-SLED"/>
    <!-- INCLUDE ignore_opensuse-bundle-gnome.xml -->
    <lock package="compiz-kde"/>
    <lock package="gtk2-engines-32bit"/>
    <lock package="kde3-i18n-de"/> <!-- !i586 -->
    <!-- INCLUDE ignore_opensuse.xml -->
    <!-- INCLUDE ignore_opensuse-cds.xml -->
    <install kind="pattern" name="default"/>
    <install kind="pattern" name="enhanced_base"/>
    <install kind="pattern" name="x11"/>
    <install kind="package" name="openSUSE-release-livecd-x11"/>
    <install kind="package" name="kernel-desktop"/>
    <install kind="package" name="bootchart"/>
    <reportProblems/>
    <showpool verbose="true"/>
  </trial>
</test>
