  </setup>
  <trial>
    <!-- ignoring some lang provides -->
    <!-- ignoring some recommends -->
    <lock package="oxygen-icon-theme-scalable"/>
    <lock package="spamassassin"/>
    <lock package="gutenprint"/>
    <lock package="rdesktop"/>
    <lock package="gimp"/>
    <lock package="ufraw"/>
    <!-- SLE themes -->
    <install kind="package" name="branding-openSUSE"/>
    <lock package="gnome-menus-branding-SLED"/>
    <lock package="gtk2-branding-SLED"/>
    <lock package="gtk2-theme-SLED"/>
    <!-- INCLUDE ignore_opensuse-bundle-gnome.xml -->
    <lock package="compiz-kde"/>
    <lock package="gtk2-engines-32bit"/>
    <lock package="kde3-i18n-de"/> <!-- !i586 -->
<lock package="vim-data"/>
<lock package="java-1_7_0-openjdk"/>
<lock package="gimp"/>
<lock package="netpbm"/>
<!-- lock package="gnome-icon-theme"/ -->
<lock package="gnome-icon-theme-extras"/>
<lock package="gnome-icon-theme-symbolic"/>
<lock package="patterns-openSUSE-yast2_basis"/>
<!-- install kind="package" name="yast2-installation"/ -->
<lock package="PackageKit"/>
<lock package="zeitgeist"/>
<!-- lock package="openSUSE-xfce-icon-theme"/ -->
<lock package="ghostscript"/>
<lock package="cups"/>
<lock package="pullin-flash-player"/>
<lock package="pullin-fluendo-mp3"/>
<install package="yast2-country"/>
<install package="yast2-trans-stats"/>

    <!-- INCLUDE ignore_opensuse.xml -->
    <!-- INCLUDE ignore_opensuse-basedon.xml -->
