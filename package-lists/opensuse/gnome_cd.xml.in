<?xml version="1.0"?>
<test>
  <setup arch="GEN_ARCH">
    <source url="GEN_URL" name="tmp"/>
    <system file="solver-system.xml"/>
    <locale name="en_US"/>
    <locale name="de"/>
    <locale name="ru"/>
    <locale name="it"/>
    <locale name="es"/>
    <locale name="pl"/>
    <locale name="fr"/>
  </setup>
  <trial>
    <!-- INCLUDE ignore_opensuse-bundle-kde.xml -->
    <lock package="gtk2-engines-32bit"/>
    <lock package="gnome2-user-docs"/> <!-- !i586 -->
    <lock package="evolution-lang"/>
    <!-- INCLUDE pdb_internals.xml -->
    <install kind="pattern" name="rest_cd_gnome"/>
    <install kind="package" name="openSUSE-release-livecd-gnome"/>
    <install kind="package" name="kernel-desktop"/> <!-- !i586 -->
    <!-- INCLUDE ignore_opensuse.xml -->
    <!-- INCLUDE ignore_opensuse-cds.xml -->
    <!-- INCLUDE ignore_opensuse-gnome-cd.xml -->
    <lock package="pam-32bit"/> <!-- !i586 !x86_64 -->
    <reportProblems/>
    <showpool verbose="true"/>
  </trial>
</test>
