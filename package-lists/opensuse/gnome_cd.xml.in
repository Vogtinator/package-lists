<?xml version="1.0"?>
<test>
  <setup arch="GEN_ARCH">
    <source url="GEN_URL" name="tmp"/>
    <system file="solver-system.xml"/>
    <locale name="en_US"/>
    <locale name="de"/>
  </setup>
  <trial>
    <!-- ignoring some lang provides -->
    <!-- ignoring some recommends -->
    <lock package="vim-data"/>
    <!-- recommended by Ooo -->
    <lock package="bundle-lang-kde-ar"/>
    <lock package="bundle-lang-kde-ca"/>
    <lock package="bundle-lang-kde-cs"/>
    <lock package="bundle-lang-kde-da"/>
    <lock package="bundle-lang-kde-de"/>
    <lock package="bundle-lang-kde-en"/>
    <lock package="bundle-lang-kde-es"/>
    <lock package="bundle-lang-kde-fi"/>
    <lock package="bundle-lang-kde-fr"/>
    <lock package="bundle-lang-kde-hu"/>
    <lock package="bundle-lang-kde-it"/>
    <lock package="bundle-lang-kde-ja"/>
    <lock package="bundle-lang-kde-ko"/>
    <lock package="bundle-lang-kde-nb"/>
    <lock package="bundle-lang-kde-nl"/>
    <lock package="bundle-lang-kde-pl"/>
    <lock package="bundle-lang-kde-pt"/>
    <lock package="bundle-lang-kde-ru"/>
    <lock package="bundle-lang-kde-sv"/>
    <lock package="bundle-lang-kde-zh"/>
    <lock package="gtk2-engines-32bit"/>
    <lock package="gnome2-user-docs"/> <!-- !i586 -->
    <lock package="evolution-lang"/>
    <!-- INTERNALS -->
    <install kind="pattern" name="rest_cd_gnome"/>
    <install kind="package" name="openSUSE-release-livecd-gnome"/>
    <!-- INCLUDE ignore_opensuse.xml -->
    <!-- INCLUDE ignore_opensuse-cds.xml -->
    <!-- INCLUDE ignore_opensuse-gnome-cd.xml -->
    <lock package="pam-32bit"/> <!-- !i586 !x86_64 -->
    <install package="gconf2-branding-openSUSE"/>
    <reportProblems/>
    <showpool verbose="true"/>
  </trial>
</test>
