<?xml version="1.0"?>
<test>
  <setup arch="GEN_ARCH">
    <source url="GEN_URL" name="tmp"/>
    <system file="solver-system.xml"/>
    <locale name="en_US"/>
  </setup>
  <trial>
    <!-- ignoring some lang provides -->
    <!-- ignoring some recommends -->
    <lock package="oxygen-icon-theme-scalable"/>
    <lock package="spamassassin"/>
    <lock package="gutenprint"/>
    <lock package="rdesktop"/>
    <lock package="gimp"/>
    <lock package="ufraw"/>
    <!-- INCLUDE ignore_opensuse-bundle-gnome.xml -->
    <lock package="compiz-kde"/>
    <lock package="gtk2-32bit"/>
    <lock name="office" kind="pattern"/>
    <lock name="OpenOffice_org" kind="package"/>
    <!-- INCLUDE ignore_opensuse.xml -->
    <!-- INCLUDE ignore_opensuse-cds.xml -->
    <!-- INCLUDE ignore_opensuse-kde-cd.xml -->
    <!-- INCLUDE pdb_internals.xml -->
    <lock package="kdelibs3"/>
    <lock package="kdelibs3-default-style"/>
    <install kind="package" name="ghostscript-library"/>
    <install kind="package" name="kernel-default"/>
    <install kind="package" name="cracklib-dict-full"/>
    <install kind="package" name="openSUSE-release-ftp"/>
    <install kind="pattern" name="kde4"/>
    <reportProblems/>
    <showpool verbose="true"/>
  </trial>
</test>
