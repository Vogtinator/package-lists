    <lock package="ufraw"/>
    <!-- SLE themes -->
    <lock package="gnome-menus-branding-SLED"/>
    <lock package="gtk2-branding-SLED"/>
    <lock package="gtk2-theme-SLED"/>
    <!-- recommended by Ooo -->
    <lock package="bundle-lang-gnome-ar"/>
    <lock package="bundle-lang-gnome-ca"/>
    <lock package="bundle-lang-gnome-cs"/>
    <lock package="bundle-lang-gnome-da"/>
    <lock package="bundle-lang-gnome-de"/>
